function [full ] = checkFullColumn( col,tc )
idx(1)=0;
idx2=cell2mat(tc(1,:));%check whereImove
idx=cat(2,idx,find(idx2==3));
count=0;
for i=1:8
    disp(i)
    disp(idx)
    if i+idx(col)<length(tc{1})
    if isequaln(tc{1}(idx(col)+i),{3})==1
      count=count+1;
    else
        count=count+1;
    end
    end
end

if count==8
    full=1;
else
    full=0;
end
end
         
                
              
